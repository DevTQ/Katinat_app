{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": false,
    "baseUrl": "./", 
    "paths": {
      "@dtos/*": ["src/dtos/*"]
    },
    "typeRoots": ["node_modules/@types", "src/types"],
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "allowJs": true
  },
  "include": ["src"]
}